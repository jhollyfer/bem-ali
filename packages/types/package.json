{
  "name": "@bem-ali/types",
  "version": "0.1.7",
  "description": "Shared Zod schemas and TypeScript types for bem-ali marketplace",
  "author": "bem-ali",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./authentication": {
      "import": {
        "types": "./dist/domains/authentication/index.d.ts",
        "default": "./dist/domains/authentication/index.js"
      },
      "require": {
        "types": "./dist/domains/authentication/index.d.cts",
        "default": "./dist/domains/authentication/index.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      },
      "require": {
        "types": "./dist/core/index.d.cts",
        "default": "./dist/core/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bem-ali",
    "types",
    "zod",
    "validation",
    "typescript"
  ]
}
